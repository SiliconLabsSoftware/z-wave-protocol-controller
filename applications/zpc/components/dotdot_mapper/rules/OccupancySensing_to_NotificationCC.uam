
// DotDot Attributes
def OCCUPANCY_SENSING_OCCUPANCY 0x04060000
def OCCUPANCY_SENSING_OCCUPANCY_SENSOR_TYPE 0x04060001
def OCCUPANCY_SENSING_OCCUPANCY_SENSOR_TYPE_BITMAP 0x04060002
def OCCUPANCY_SENSING_PIR_OCCUPIED_TO_UNOCCUPIED_DELAY 0x04060010
def OCCUPANCY_SENSING_PIR_UNOCCUPIED_TO_OCCUPIED_DELAY 0x04060011
def OCCUPANCY_SENSING_PIR_UNOCCUPIED_TO_OCCUPIED_THRESHOLD 0x04060012
def OCCUPANCY_SENSING_ULTRASONIC_OCCUPIED_TO_UNOCCUPIED_DELAY 0x04060020
def OCCUPANCY_SENSING_ULTRASONIC_UNOCCUPIED_TO_OCCUPIED_DELAY 0x04060021
def OCCUPANCY_SENSING_ULTRASONIC_UNOCCUPIED_TO_OCCUPIED_THRESHOLD 0x04060022
def OCCUPANCY_SENSING_PHYSICAL_CONTACT_OCCUPIED_TO_UNOCCUPIED_DELAY 0x04060030
def OCCUPANCY_SENSING_PHYSICAL_CONTACT_UNOCCUPIED_TO_OCCUPIED_DELAY 0x04060031
def OCCUPANCY_SENSING_PHYSICAL_CONTACT_UNOCCUPIED_TO_OCCUPIED_THRESHOLD 0x04060032

// Z-Wave Attributes
def zwSUPPORTED_NOTIFICATION_TYPES              0x7102
def zwNOTIFICATION_TYPE                         0x7103
def zwNOTIFICATION_SUPPORTED_STATES_OR_EVENTS   0x7104
def zwNOTIFICATION_STATE                        0x7105
def zwNOTIFICATION_EVENT                        0x7106
def zwNOTIFICATION_EVENT_PARAMETERS             0x7107

// State Idle, applies for all Notification types.
def NOTIFICATION_STATE_IDLE 0x00
// Unknown state/event, applies for all Notification types.
def NOTIFICATION_UNKNOWN_STATE_EVENT 0xFE

///< Home Security Notification Type
def NOTIFICATION_TYPE_HOME_SECURITY 0x07
///< Access Control Notification Type
def NOTIFICATION_TYPE_ACCESS_CONTROL 0x06

// Z-Wave notification states (see zwave_command_class_notification.h)
def NOTIFICATION_STATE_HOME_SECURITY_SENSOR_STATUS        0x00
def NOTIFICATION_STATE_HOME_SECURITY_MOTION_SENSOR_STATUS 0x02
def NOTIFICATION_STATE_ACCESS_CONTROL_DOOR_STATE          0x02
def NOTIFICATION_STATE_ACCESS_CONTROL_DOOR_HANDLE_STATE   0x03

def NOTIFICATION_STATE_IDLE 0x00
def NOTIFICATION_HOME_SECURITY_STATE_INTRUSION_WITH_LOCATION 0x01
def NOTIFICATION_HOME_SECURITY_STATE_INTRUSION 0x02
def NOTIFICATION_ACCESS_CONTROL_STATE_WINDOW_DOOR_OPEN 0x16
def NOTIFICATION_ACCESS_CONTROL_STATE_WINDOW_DOOR_CLOSE 0x17

///< Product cover removed state
def NOTIFICATION_HOME_SECURITY_TAMPERING 0x03
def NOTIFICATION_HOME_SECURITY_STATE_MOTION_DETECTION_WITH_LOCATION 0x07
def NOTIFICATION_HOME_SECURITY_STATE_MOTION_DETECTION 0x08
def NOTIFICATION_HOME_SECURITY_STATE_MAGNETIC_INTERFERENCE 0x0B

// Occupancy Sensing to Motion
scope 10 {
  r'OCCUPANCY_SENSING_OCCUPANCY =
    if (r'zwNOTIFICATION_TYPE[NOTIFICATION_TYPE_HOME_SECURITY].zwNOTIFICATION_STATE[NOTIFICATION_STATE_HOME_SECURITY_MOTION_SENSOR_STATUS].zwNOTIFICATION_EVENT == NOTIFICATION_HOME_SECURITY_STATE_MOTION_DETECTION_WITH_LOCATION) 1
    if (r'zwNOTIFICATION_TYPE[NOTIFICATION_TYPE_HOME_SECURITY].zwNOTIFICATION_STATE[NOTIFICATION_STATE_HOME_SECURITY_MOTION_SENSOR_STATUS].zwNOTIFICATION_EVENT == NOTIFICATION_HOME_SECURITY_STATE_MOTION_DETECTION) 1
    if (r'zwNOTIFICATION_TYPE[NOTIFICATION_TYPE_HOME_SECURITY].zwNOTIFICATION_STATE[NOTIFICATION_STATE_HOME_SECURITY_SENSOR_STATUS].zwNOTIFICATION_EVENT == NOTIFICATION_HOME_SECURITY_STATE_INTRUSION) 1
    if (r'zwNOTIFICATION_TYPE[NOTIFICATION_TYPE_HOME_SECURITY].zwNOTIFICATION_STATE[NOTIFICATION_STATE_HOME_SECURITY_SENSOR_STATUS].zwNOTIFICATION_EVENT == NOTIFICATION_HOME_SECURITY_STATE_INTRUSION_WITH_LOCATION) 1
    if (r'zwNOTIFICATION_TYPE[NOTIFICATION_TYPE_ACCESS_CONTROL].zwNOTIFICATION_STATE[NOTIFICATION_STATE_ACCESS_CONTROL_DOOR_STATE].zwNOTIFICATION_EVENT == NOTIFICATION_ACCESS_CONTROL_STATE_WINDOW_DOOR_OPEN) 1
    if (r'zwNOTIFICATION_TYPE[NOTIFICATION_TYPE_HOME_SECURITY].zwNOTIFICATION_STATE[NOTIFICATION_STATE_HOME_SECURITY_MOTION_SENSOR_STATUS].zwNOTIFICATION_EVENT == NOTIFICATION_STATE_IDLE) 0
    if (r'zwNOTIFICATION_TYPE[NOTIFICATION_TYPE_HOME_SECURITY].zwNOTIFICATION_STATE[NOTIFICATION_STATE_HOME_SECURITY_SENSOR_STATUS].zwNOTIFICATION_EVENT == NOTIFICATION_STATE_IDLE) 0
    if (r'zwNOTIFICATION_TYPE[NOTIFICATION_TYPE_ACCESS_CONTROL].zwNOTIFICATION_STATE[NOTIFICATION_STATE_ACCESS_CONTROL_DOOR_STATE].zwNOTIFICATION_EVENT == NOTIFICATION_ACCESS_CONTROL_STATE_WINDOW_DOOR_CLOSE) 0
    undefined

  // TODO we should really check the zwNOTIFICATION_SUPPORTED_STATES_OR_EVENTS here
  // but this requires changes to the mapper engine as zwNOTIFICATION_SUPPORTED_STATES_OR_EVENTS is not an integer but a blob
  r'OCCUPANCY_SENSING_OCCUPANCY_SENSOR_TYPE =
    if (e'zwNOTIFICATION_TYPE[NOTIFICATION_TYPE_HOME_SECURITY].zwNOTIFICATION_STATE[NOTIFICATION_STATE_HOME_SECURITY_MOTION_SENSOR_STATUS].zwNOTIFICATION_EVENT) 0
    if (e'zwNOTIFICATION_TYPE[NOTIFICATION_TYPE_HOME_SECURITY].zwNOTIFICATION_STATE[NOTIFICATION_STATE_HOME_SECURITY_SENSOR_STATUS].zwNOTIFICATION_EVENT) 0
    if (e'zwNOTIFICATION_TYPE[NOTIFICATION_TYPE_ACCESS_CONTROL].zwNOTIFICATION_STATE[NOTIFICATION_STATE_ACCESS_CONTROL_DOOR_STATE].zwNOTIFICATION_EVENT) 3
    undefined

  r'OCCUPANCY_SENSING_OCCUPANCY_SENSOR_TYPE_BITMAP =
    if (e'zwNOTIFICATION_TYPE[NOTIFICATION_TYPE_HOME_SECURITY].zwNOTIFICATION_STATE[NOTIFICATION_STATE_HOME_SECURITY_MOTION_SENSOR_STATUS].zwNOTIFICATION_EVENT) 1
    if (e'zwNOTIFICATION_TYPE[NOTIFICATION_TYPE_HOME_SECURITY].zwNOTIFICATION_STATE[NOTIFICATION_STATE_HOME_SECURITY_SENSOR_STATUS].zwNOTIFICATION_EVENT) 1
    if (e'zwNOTIFICATION_TYPE[NOTIFICATION_TYPE_ACCESS_CONTROL].zwNOTIFICATION_STATE[NOTIFICATION_STATE_ACCESS_CONTROL_DOOR_STATE].zwNOTIFICATION_EVENT) 4
    undefined
}
